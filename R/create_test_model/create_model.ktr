<script>library&#x28;lattice&#x29;&#xd;&#xa;library&#x28;latticeExtra&#x29;&#xd;&#xa;library&#x28;data.table&#x29;&#xd;&#xa;library&#x28;ggplot2&#x29;&#xd;&#xa;library&#x28;hexbin&#x29;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;setwd&#x28;&#x22;c&#x3a;&#x2f;data&#x2f;pentaho&#x2f;r_demo&#x22;&#x29;&#xd;&#xa;&#xd;&#xa;data&#x28;diamonds&#x29;&#xd;&#xa;&#xd;&#xa;diamonds&#x24;logprice &#x3c;- log10&#x28;diamonds&#x24;price&#x29;&#xd;&#xa;diamonds&#x24;logcarat &#x3c;- log10&#x28;diamonds&#x24;carat&#x29;&#xd;&#xa;&#xd;&#xa;nr &#x3c;- nrow&#x28;diamonds&#x29;&#xd;&#xa;&#xd;&#xa;samp &#x3c;- sample&#x28;nr,nr&#x29;&#xd;&#xa;samp_train &#x3c;- samp&#x5b;1&#x3a;&#x28;.8&#x2a;nr&#x29;&#x5d;&#xd;&#xa;samp_test &#x3c;- samp&#x5b;&#x28;.8&#x2a;nr&#x2b;1&#x29;&#x3a;nr&#x5d;&#xd;&#xa;&#xd;&#xa;diamonds_train &#x3c;- diamonds&#x5b;samp_train,&#x5d;&#xd;&#xa;diamonds_test &#x3c;- diamonds&#x5b;samp_test,&#x5d;&#xd;&#xa;&#xd;&#xa;lmobj &#x3c;- lm&#x28;logprice&#x7e;logcarat&#x2b;I&#x28;cut&#x29;&#x2b;I&#x28;color&#x29;&#x2b;I&#x28;clarity&#x29;, data&#x3d;diamonds_train&#x29;&#xd;&#xa;&#xd;&#xa;diamonds_train&#x24;predicted_logprice &#x3c;- predict&#x28;lmobj,newdata&#x3d;diamonds_train&#x29;&#xd;&#xa;&#xd;&#xa;hexplot &#x3c;- function&#x28;df,hdr&#x3d;&#x22;Train&#x22;&#x29;&#xd;&#xa;&#x7b;&#xd;&#xa;	hpplt &#x3c;- hexbinplot&#x28;logprice&#x7e;predicted_logprice, &#xd;&#xa;		data&#x3d;df,type&#x3d;c&#x28;&#x22;g&#x22;,&#x22;r&#x22;&#x29;,col&#x3d;&#x22;red&#x22;,lwd&#x3d;5,aspect&#x3d;.7,&#xd;&#xa;		colorkey&#x3d;FALSE, main&#x3d;paste&#x28;&#x22;Diamonds Data&#x5c;n&#x22;, hdr, sep&#x3d;&#x22;&#x22;&#x29;,&#xd;&#xa;		&#x29;&#xd;&#xa;	return&#x28;hpplt&#x29;&#xd;&#xa;&#x7d;&#xd;&#xa;&#xd;&#xa;save&#x28;hexplot,lmobj,file&#x3d;&#x22;diamonds.rdata&#x22;&#x29;&#xd;&#xa;&#xd;&#xa;diamonds_test</script>
