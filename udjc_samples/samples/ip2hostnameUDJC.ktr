        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.net.InetAddress;
import java.net.UnknownHostException;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
   //  Rows of data
   Object[] r = getRow();
 
    //  If there are no more rows, clean up and report completed
    if (r == null) {
        setOutputDone();
        return false;
    }
    // Create an output row for each input row we find
    r = createOutputRow(r, data.outputRowMeta.size());
    // Get the value from an input field
    String test = get(Fields.In, "host").getString(r);

    // Try to get the Name by host
	try
	{
		InetAddress ia = InetAddress.getByName(test); 

		//   Get the name from the InetAddress object
		String nameVal = ia.getCanonicalHostName();

	    // Set a value in a new output field
    	get(Fields.Out, "resolved").setValue(r, nameVal);

	    // Send the row on to the next step.
    	putRow(data.outputRowMeta, r);
	}
	catch (UnknownHostException e)
	{
		// Dump the trace to launcher
		e.printStackTrace();
	}

    return true;
}


]]></class_source>
